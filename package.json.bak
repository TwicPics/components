{
    "dependencies": {
        "@angular/common": "13.3.7",
        "@angular/core": "13.3.7",
        "@nuxt/kit": "3.0.0-rc.3",
        "@rollup/plugin-node-resolve": "13.3.0",
        "@rollup/plugin-typescript": "8.3.2",
        "@rollup/pluginutils": "4.2.1",
        "@tsconfig/svelte": "3.0.0",
        "@types/prop-types": "15.7.5",
        "@types/react": "18.0.9",
        "@vue/compiler-sfc": "3.2.33",
        "fs-extra": "10.1.0",
        "magic-string": "0.26.2",
        "markdown-include": "^0.4.3",
        "nuxt": "3.0.0-rc.1",
        "replace-in-file": "6.3.2",
        "rollup": "2.72.1",
        "rollup-plugin-css-porter": "1.0.2",
        "rollup-plugin-dts": "4.2.1",
        "rollup-plugin-import-css": "3.0.3",
        "rollup-plugin-svelte": "7.1.0",
        "rollup-plugin-terser": "7.0.2",
        "rollup-plugin-vue": "6.0.0",
        "rollup-plugin-vue2": "0.8.1",
        "svelte": "3.48.0",
        "svelte-preprocess": "4.10.6",
        "typescript": "4.6.4",
        "uglify-js": "3.15.5",
        "vue": "3.2.33",
        "vue-typescript-import-dts": "4.0.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "5.23.0",
        "@typescript-eslint/parser": "5.23.0",
        "eslint": "8.15.0",
        "eslint-config-creative-area": "0.11.1",
        "eslint-plugin-react": "7.29.4",
        "eslint-plugin-vue": "8.7.1",
        "npm-run-all": "4.1.5"
    },
    "eslintConfig": {
        "env": {
            "browser": true
        },
        "extends": [
            "eslint:recommended",
            "creative-area/es2020/module"
        ],
        "overrides": [
            {
                "files": [
                    "*.jsx",
                    "*.svelte",
                    "*.ts",
                    "*.tsx",
                    "*.vue"
                ],
                "extends": [
                    "plugin:@typescript-eslint/eslint-recommended",
                    "plugin:@typescript-eslint/recommended",
                    "plugin:react/recommended",
                    "plugin:vue/vue3-recommended",
                    "creative-area/es2020/module"
                ],
                "rules": {
                    "@typescript-eslint/no-shadow": [
                        "error"
                    ],
                    "@typescript-eslint/no-unused-vars": [
                        "error"
                    ],
                    "@typescript-eslint/no-use-before-define": [
                        "error"
                    ],
                    "no-use-before-define": "off",
                    "prefer-named-capture-group": "off",
                    "vue/comment-directive": "off",
                    "vue/multi-word-component-names": "off",
                    "vue/html-indent": [
                        "error",
                        4
                    ]
                },
                "settings": {
                    "react": {
                        "version": "17"
                    }
                }
            }
        ],
        "parser": "vue-eslint-parser",
        "parserOptions": {
            "parser": "@typescript-eslint/parser"
        },
        "plugins": [
            "@typescript-eslint"
        ],
        "rules": {
            "prefer-named-capture-group": "off",
            "new-cap": [
                "error",
                {
                    "capIsNewExceptions": [
                        "Directive",
                        "HostBinding",
                        "HostListener",
                        "Injectable",
                        "Input",
                        "NgModule",
                        "Output",
                        "Pipe",
                        "ViewChild",
                        "ViewChildren",
                        "Component"
                    ]
                }
            ]
        }
    },
    "license": "MIT",
    "scripts": {
        "build": "$npm_execpath install && node build/documentation.js && node build",
        "lint": "eslint .",
        "postinstall": "node build/angular/angularInstaller.js && node build/postinstall.js",
        "documentation": "node build/documentation.js"
    },
    "type": "module"
}
