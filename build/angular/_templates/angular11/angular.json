{
    "version": 1,
    "projects": {
        "lib": {
            "projectType": "library",
            "root": "lib",
            "sourceRoot": "lib/src",
            "prefix": "",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "project": "lib/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "lib/tsconfig.lib.prod.json"
                        }
                    },
                    "defaultConfiguration": "production"
                }
            }
        }
    }
}